{
  "rewrites": [
    {
      "source": "/api/bored/:path*",
      "destination": "https://bored-api.appbrewery.com/:path*"
    },
    {
      "source": "/api/jokeapi/:path*",
      "destination": "https://v2.jokeapi.dev/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

